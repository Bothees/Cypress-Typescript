{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "allowJs": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["es2022", "dom"],
    "types": ["cypress", "node"],
  },
  "include": ["cypress"],
  "exclude": ["node_modules"]
}
