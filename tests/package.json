{
  "name": "cypress",
  "private": true,
  "scripts": {
    "cypress:runner": "cypress open",
    "cypress:run": "cypress run",
    "cypress:clean": "ts-node cypress_clean.ts",
    "cypress:report": "ts-node cypress_report.ts",
    "cypress:run:report": "npm run cypress:clean; npm run cypress:run ; npm run cypress:report",
    "cypress:run:singleTest": "npx cypress run --env TAGS='@test'",
    "cy:parallel": "cypress-parallel -s cypress:run -t 3 -d cypress/features/ -m false"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@faker-js/faker": "^8.4.1",
    "@types/fs-extra": "^11.0.4",
    "cypress": "^13.11.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-cucumber-tagging": "^1.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "fast-xml-parser": "^4.4.0",
    "resolve": "^1.22.1",
    "typescript": "^4.9.5",
    "@badeball/cypress-cucumber-preprocessor": "^20.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.1",
    "cypress-parallel": "^0.14.0",
    "fs": "0.0.1-security",
    "fs-extra": "^11.2.0",
    "ls": "^0.2.1",
    "multiple-cucumber-html-reporter": "^3.6.2",
    "path": "^0.12.7"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "filterSpecs": true,
    "omitFiltered": true,
    "step_definitions": "cypress/support/step_definitions/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}